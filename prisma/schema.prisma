generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model BoundChat {
  chatId    BigInt   @id
  userId    BigInt?
  createdAt DateTime @default(now())
}

model Plan {
  id        Int      @id @default(autoincrement())
  hash      String   @unique
  file      Bytes
  image     Bytes?
  createdAt DateTime @default(now())
}
