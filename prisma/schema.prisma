generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model BoundChat {
  id     Int @id @default(autoincrement())
  chatId Int
  userId Int

  @@unique([chatId, userId])
}

model Plan {
  id        Int      @id @default(autoincrement())
  hash      String   @unique
  file      Bytes
  image     Bytes?
  createdAt DateTime @default(now())
}
